{
  "name": "react-digraph",
  "description": "directed graph react component",
  "version": "7.0.0",
  "keywords": [
    "uber-library",
    "babel",
    "es6",
    "d3",
    "react",
    "graph",
    "digraph"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uber/react-digraph.git"
  },
  "bugs": {
    "url": "https://github.com/uber/react-digraph/issues/new",
    "email": "antonb@uber.com"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "license": "MIT",
  "main": "dist/main.min.js",
  "types": "./typings/index.d.ts",
  "peerDependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "dependencies": {
    "d3": "^5.7.0",
    "dagre": "^0.8.2",
    "fast-deep-equal": "^2.0.1",
    "html-react-parser": "^0.6.1",
    "kld-affine": "9renpoto/kld-affine#development",
    "kld-intersections": "^0.7.0",
    "line-intersect": "^2.1.1",
    "svg-intersections": "^0.4.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@types/d3": "^5.16.3",
    "@types/dagre": "^0.7.44",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/svg-intersections": "^0.4.0",
    "babel-loader": "^8.1.0",
    "brace": "^0.11.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^4.3.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^7.10.0",
    "eslint-config-fusion": "^6.0.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cup": "^2.0.1",
    "eslint-plugin-es6-recommended": "^0.1.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-import-order": "^2.1.4",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^2.4.0",
    "jest": "^26.5.0",
    "jsdom": "^11.12.0",
    "lint-staged": "^8.2.0",
    "live-server": "^1.2.0",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "opn-cli": "3.1.0",
    "prettier": "^2.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.12.0",
    "react-ace": "^6.1.4",
    "react-dom": "^16.12.0",
    "react-router-dom": "^4.3.1",
    "rimraf": "^2.7.1",
    "sass-loader": "^10.0.2",
    "source-map-loader": "^1.1.0",
    "style-loader": "^1.3.0",
    "svg-inline-loader": "^0.8.0",
    "terser-webpack-plugin": "^4.2.2",
    "typescript": "^4.0.3",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack --config webpack.prod.js",
    "clean": "rimraf ./dist",
    "watch": "webpack --watch",
    "build-css": "node-sass --include-path scss src/styles/main.scss dist/styles/main.css && node-sass --include-path scss src/examples/app.scss dist/examples/app.css",
    "cover": "npm run test",
    "example": "npm run serve",
    "flow": "flow .",
    "jenkins-install": "npm install",
    "jenkins-jshint": "npm run lint -- --o=jshint.xml --f=checkstyle",
    "jenkins-test": "npm run jenkins-jshint && npm run test",
    "live-server": "live-server ./dist --entry-file=./index.html",
    "live-serve": "npm-run-all --parallel watch live-server",
    "lint": "eslint src",
    "lint-fix": "eslint --fix src",
    "precommit": "lint-staged && npm run test",
    "prefast-test": "npm run prepublish",
    "serve": "npm run live-serve",
    "test": "jest",
    "test:debug": "node --inspect node_modules/.bin/jest --watch --runInBand",
    "view-cover": "npm run cover -- --report=html && opn ./coverage/index.html",
    "package": "npm-run-all clean lint test build build:prod",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "jest": {
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transformIgnorePatterns": [
      "node_modules"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)"
    ],
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "collectCoverageFrom": [
      "**/src/**/*.{js,ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/*.d.ts",
      "!**/examples/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html",
      "cobertura"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/__mocks__/styles.mock.js",
      "@fortawesome/fontawesome-free/svgs/solid/expand.svg": "<rootDir>/__mocks__/icon.mock.js"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "prettier": {
    "singleQuote": true
  }
}
